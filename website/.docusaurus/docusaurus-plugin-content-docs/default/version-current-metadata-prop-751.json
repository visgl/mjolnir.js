  "docs": {
    "api-reference/event-manager": {
      "id": "api-reference/event-manager",
      "title": "EventManager",
      "description": "Provides a unified API for subscribing to events about both basic input events (e.g. 'pointermove', 'keydown', 'wheel') and gestural input (e.g. 'click', 'tap', 'panstart').",
      "sidebar": "docsSidebar"
    },
    "api-reference/pan": {
      "id": "api-reference/pan",
      "title": "Pan",
      "description": "Recognized when the pointer is down and moved in the allowed direction.",
      "sidebar": "docsSidebar"
    },
    "api-reference/pinch": {
      "id": "api-reference/pinch",
      "title": "Pinch",
      "description": "Recognized when two or more pointers are moving toward (zoom-in) or away from each other (zoom-out).",
      "sidebar": "docsSidebar"
    },
    "api-reference/press": {
      "id": "api-reference/press",
      "title": "Press",
      "description": "Recognized when the pointer is down for some time without any movement.",
      "sidebar": "docsSidebar"
    },
    "api-reference/rotate": {
      "id": "api-reference/rotate",
      "title": "Rotate",
      "description": "Recognized when two or more pointer are moving in a circular motion.",
      "sidebar": "docsSidebar"
    },
    "api-reference/swipe": {
      "id": "api-reference/swipe",
      "title": "Swipe",
      "description": "Recognized when the pointer is moving fast (velocity), with enough distance in the allowed direction.",
      "sidebar": "docsSidebar"
    },
    "api-reference/tap": {
      "id": "api-reference/tap",
      "title": "Tap",
      "description": "Recognized when the pointer is doing a small tap/click. Multiple taps are recognized if they occur between the given interval and position. The eventData from the emitted event contains the property tapCount, which contains the amount of multi-taps being recognized.",
      "sidebar": "docsSidebar"
    },
